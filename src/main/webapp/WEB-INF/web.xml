<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
    <servlet>
        <servlet-name>articles</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.ArticlesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lag</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.LagServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datastore</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.DatastoreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datastoreMagasin</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.DatastoreServletAncestor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datastoreXP</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.DatastoreServletExperiementations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>enqueue</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.Enqueue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>handler</servlet-name>
        <servlet-class>fr.unice.polytech.si5.cc.jcancela.Handler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lag</servlet-name>
        <url-pattern>/lag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>articles</servlet-name>
        <url-pattern>/articles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>enqueue</servlet-name>
        <url-pattern>/push</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datastore</servlet-name>
        <url-pattern>/datastore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datastoreMagasin</servlet-name>
        <url-pattern>/datastoreMagasin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datastoreXP</servlet-name>
        <url-pattern>/datastoreXP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>handler</servlet-name>
        <url-pattern>/worker</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
